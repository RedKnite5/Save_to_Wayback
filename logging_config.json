{
	"version": 1,
	"formatters": {
		"files": {
			"()": "savetowayback.FileFormatter"
		},
		"shorten_urls": {
			"()": "savetowayback.STDOutFormatter",
			"datefmt": "%b-%d %H:%M:%S"
		}
	},
	"handlers": {
		"stdout": {
			"class": "logging.StreamHandler",
			"level": "INFO",
			"formatter": "shorten_urls",
			"stream": "ext://sys.stdout"
		},
		"file": {
			"class": "logging.handlers.RotatingFileHandler",
			"level": "DEBUG",
			"formatter": "files",
			"filename": "data/urls_saved.log",
			"maxBytes": 5000000,
			"backupCount": 10
		}
	},
	"loggers": {
		"root": {
			"level": "DEBUG",
			"handlers": ["stdout", "file"]
		}
	}
}